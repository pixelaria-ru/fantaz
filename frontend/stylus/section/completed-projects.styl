.section--completed-projects
  align-items center
  padding 20px 85px 80px

  .complete-project
    max-width: 33%;
    margin-left: 50px;
    transition .33s
    cursor pointer
    display none
    &.active
      display block
    &:hover
      transform scale(1.05)
    img
      width max-content
      margin-bottom 15px
      height: 100px;
      object-fit: contain;

    h3
      color #444444
      font-size 18px
      font-weight 400
      line-height 27px
  h2
    color #444444
    font-size 18px
    font-weight 700
    line-height 28px
    border-right 1px solid #dedede
    padding-right 50px
    margin-right 35px
    width min-content
    display flex
    text-align left
    align-items center
    height 270px
    
