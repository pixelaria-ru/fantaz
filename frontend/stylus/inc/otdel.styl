.initi__container-righter
  width 380px
  height 420px
  background-color #ffffff
  padding 50px 30px
  z-index: 9;
  margin-top -125px
  position relative
  &:after
    content ''
    position absolute
    border-right-color #56b3ca
    border-top-color #56b3ca
    border 35px solid transparent
    top 0
    right 0
  &:before
    content ''
