.discription-program
  box-shadow 0 0 10px rgba(0, 0, 0, 0.1)
  background-color #ffffff
  margin-bottom 25px
  .left-side
    align-items center
    justify-content center
    width 260px

    background-color #777777
    flex-direction column
    &-program__img
      width 100px
      height 100px
      align-items center
      justify-content center
      background-color #ffffff
      border-radius 50%
      display flex
      img
        max-width 50px
        max-height 50px
        object-fit: contain;
    h3
      color #ffffff
      margin-top 20px
      font-size 24px
      font-weight 400
      max-width 210px
      text-align center
      line-height 28px
  .right-side
    &-program__first-line
      width 850px
      height 240px
      background-color #4eabc2
      padding 50px 40px
      h4
        color #ffffff
        font-size 18px
        font-weight 700
        line-height 28px
        margin-bottom 12px

      .first-line__item
        color #ffffff
        font-size 18px
        font-weight 400
        margin-bottom 10px
        line-height 28px
        display flex
        align-items center
        &:nth-child(1)
          &:before
            content ''
            display block
            background-image url('../images/professional-advance.png') no-repeat
            width 20px
            height 20px
            margin-right 10px
        &:nth-child(2)
          &:before
            content ''
            display block
            background-image url('../images/mission.png') no-repeat
            width 20px
            height 20px
            margin-right 10px
        &:nth-child(3)
          &:before
            content ''
            display block
            background-image url('../images/collaboration.png') no-repeat
            width 20px
            height 20px
            margin-right 10px
    &-program__second-line
      padding 50px 40px
      h4
        color #444444
        font-size 16px
        font-weight 700
        line-height 28px
        margin-bottom 12px
      .second-line__item
        color #444444
        font-size 16px
        font-weight 400
        line-height 28px
        margin-bottom 10px
